<div class="mix-index album ">
  <div class="row">
    <div class="col-md-12 text-center">
    <%=render partial: 'users/user', locals: {user: @user}%>
    </div>
  </div>
  <div class="row">
    <div class="col-md-12 text-center">
      <h2>User Mixes</h2> 
      <h3><%=new_link%></h3>
      <div class="album py-5" style="width: 80%; margin: auto">
        <%@mixes.each do |m|%>
          <%=render partial: 'mix_preview', locals: {mix: m}%>
        <%end%>
      </div>
      <h3>Liked Mixes</h3>
      <div class="album py-5" style="width: 80%; margin: auto">
        <%@liked_mixes.each do |m|%>
          <%=render partial: 'mix_preview', locals: {mix: m}%>
        <%end%>
      </div>
      <br>
    </div>
  </div>
</div>